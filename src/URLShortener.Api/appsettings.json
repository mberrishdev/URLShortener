{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Debug"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "ConnectionStrings": {
    "URLShortener": "Server=(local);Database=URLShortener; Trusted_Connection=True;TrustServerCertificate=True; Integrated security=false"
    "Redis": "localhost:6379"
  },
  "RedisCache": {
    "InstanceName": "URLShortener:"
  },
  "CorsAllowedOrigins": "*"
}